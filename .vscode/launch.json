{
	"version": "0.2.0",
	"configurations": [{
		"name": "[Node] Current file",
		"type": "node",
		"request": "launch",
		"args": ["${relativeFile}"],
		"runtimeArgs": [
			"--nolazy",
			"-r", "dotenv/config",
			"-r", "@player1os/javascript-support/lib/runtime"
		],
		"cwd": "${workspaceRoot}",
		"protocol": "inspector",
		"windows": {
			"runtimeExecutable": "C:\\Programs\\nodejs\\node"
		}
	}]
}
